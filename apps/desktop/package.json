{
  "name": "@botsapp/desktop",
  "prviate": true,
  "productName": "Bots",
  "version": "0.1.14",
  "description": "An app to make personalized AI bots and chat with them",
  "repository": {
    "type": "git",
    "url": "https://github.com/withbotshq/botsapp.git"
  },
  "main": "./.webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "package:copy": "cp -R out/stable/Bots-darwin-*/Bots.app/ /Applications/Bots.app",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "check": "konk run c -gL -n'check:*'",
    "check:build": "tsc --noEmit",
    "check:format": "prettier --check .",
    "check:lint": "eslint .",
    "fix": "konk run s -L -n'fix:*'",
    "fix:format": "prettier --write . --list-different"
  },
  "keywords": [],
  "author": {
    "name": "Jonathan Clem",
    "email": "j@jclem.me"
  },
  "license": "MIT",
