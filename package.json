{
  "name": "bots-app",
  "productName": "Bots",
  "version": "0.1.2",
  "description": "An app to make personalized AI bots and chat with them",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "konk run s -L -n'package:*'",
    "package:build": "electron-forge package",
    "package:copy": "cp -R out/stable/Bots-darwin-*/Bots.app/ /Applications/Bots.app",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "check": "konk run c -gL -n'check:*'",
    "check:build": "tsc --noEmit",
    "check:format": "prettier --check .",
    "check:lint": "eslint .",
    "fix": "konk run s -L -n'fix:*'",
    "fix:format": "prettier --write . --list-different"
  },
  "keywords": [],
  "author": {
    "name": "Jonathan Clem",
    "email": "j@jclem.me"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-forge/plugin-vite": "^6.1.1",
    "@electron-forge/publisher-github": "^6.1.1",
    "@electron/rebuild": "^3.2.10",
    "@types/better-sqlite3": "^7.6.4",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/sql.js": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.0.3",
    "electron": "^24.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "konk": "^0.27.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@jclem/assert": "^1.1.0",
    "@tanstack/react-query": "^4.28.0",
    "date-fns": "^2.29.3",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.6",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^3.0.1",
    "vite": "^4.2.1",
    "zod": "^3.21.4"
  }
}
